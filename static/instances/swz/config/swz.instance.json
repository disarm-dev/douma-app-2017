{
  "applets": {
    "irs_monitor": {
      "map": {
        "aggregation_names": [
          "structures sprayed %"
        ]
      },
      "table": {
        "chart_type": "table",
        "bin_by": "location.selection.name",
        "aggregation_names": [
          "structures sprayed", 
          "structures sprayed %"
        ]
      },
      "charts": [
        {
          "id": "structures_found",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Structures found %",
              "yaxis": {
                "title": "% structures found"
              },
              "xaxis": {
                "title": "Period commencing"
              }
            },
            "chart_type": "line",
            "time_series": true,
            "bin_by": "recorded_on",
            "multi_series": [{
              "aggregation_name": "structures found %"
            }]
          }
        },
        {
          "id": "structures_sprayed",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Structures sprayed %",
              "yaxis": {
                "title": "% structures sprayed"
              },
              "xaxis": {
                "title": "Period commencing"
              }
            },
            "chart_type": "line",
            "time_series": true,
            "bin_by": "recorded_on",
            "multi_series": [{
              "aggregation_name": "structures sprayed %"
            }]
          }
        },
        {
          "id": "distribution_sprayed_versus_visited",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Distribution of locked vs sprayed of structures visited",
              "yaxis": {
                "title": "%"
              },
              "xaxis": {
                "title": "Period commencing"
              }
            },
            "chart_type": "line",
            "time_series": true,
            "bin_by": "recorded_on",
            "multi_series": [
              {
                "aggregation_name": "structures sprayed %"
              }, 
              {
                "aggregation_name": "structures found %"
              } 
            ]
          }
        },
        {
          "id": "proportion_of_population_covered_of_structures_visited",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Proportion of population covered of structures visited",
              "yaxis": {
                "title": "%"
              },
              "xaxis": {
                "title": "Period commencing"
              }
            },
            "chart_type": "line",
            "time_series": true,
            "bin_by": "recorded_on",
            "multi_series": [{
              "aggregation_name": "structures found %"
            }]
          }
        },
        {
          "id": "distribution_of_locked_versus_sprayed_per_con",
          "style": {
            "height_constraint": "none",
            "width_constraint": "full"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Locked % vs. sprayed % per consituency",
              "yaxis": {
                "title": "%"
              },
              "xaxis": {
                "title": "Constituencies"
              },
              "barmode": "stack"
            },
            "chart_type": "bar",
            "bin_by": "location.selection.name",
            "multi_series": [
              {
                "aggregation_name": "structures sprayed %",
                "colour": "green"
              },
              {
                "aggregation_name": "sprayable structures not sprayed %",
                "colour": "red"
              }
            ]
          }
        }
      ]
    },
    "irs_plan": {},
    "irs_record_point": {},
    "irs_tasker": {},
    "meta": {},
    "data_wizard": {},
    "debug": {}
  },
  "map_focus": {
    "centre": {
      "lat": -26.502530898533244,
      "lng": 31.528015136718754
    },
    "zoom": 7.5
  },
  "instance": {
    "title": "Swaziland IRS Tool",
    "location_name": "Swaziland",
    "slug": "swz"
  },
  "spatial_hierarchy": {
    "data_version": 1,
    "markers": {
      "planning_level_name": "localities",
      "record_location_selection_level_name": "localities",
      "denominator_fields": {
        "number_of_structures": "NmStrct"
      }
    },
    "levels": [
      {
        "group_by_field": "Finl_rg",
        "field_name": "AggUniCode",
        "display_field_name": "Fnl_l_1",
        "name": "localities"
      },
      {
        "field_name": "ClstrID",
        "display_field_name": "ClstrID",
        "name": "clusters"
      }
    ]
  }
}
