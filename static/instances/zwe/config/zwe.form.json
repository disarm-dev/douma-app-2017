{
 "pages": [
  {
   "name": "page1",
   "elements": [
    {
     "type": "text",
     "isRequired": true,
     "name": "household_code",
     "title": "Household code"
    },
    {
     "type": "radiogroup",
     "name": "sprayed",
     "title": "Household sprayed",
     "isRequired": true,
     "choices": [
      {
       "value": "yes",
       "text": "Yes"
      },
      {
       "value": "partial",
       "text": "Partially Sprayed"
      },
      {
       "value": "no",
       "text": "No"
      }
     ]
    },
    {
     "type": "radiogroup",
     "name": "failure_reason",
     "visible": false,
     "visibleIf": "{sprayed} = 'no' or {sprayed} = 'partial'",
     "title": "Reason for failure to fully spray household",
     "isRequired": true,
     "choices": [
      {
       "value": "locked",
       "text": "Locked"
      },
      {
       "value": "refused",
       "text": "Refused"
      }
     ]
    },
    {
     "type": "checkbox",
     "name": "reasons_notspraying",
     "visible": false,
     "visibleIf": "{failure_reason} = 'refused'",
     "title": "Reason for refusal",
     "isRequired": true,
     "hasOther": true,
     "choices": [
      {
       "value": "food_storage",
       "text": "Food Storage"
      },
      {
       "value": "infant",
       "text": "Infant"
      },
      {
       "value": "kitchen",
       "text": "Kitchen"
      }
     ]
    },
    {
     "type": "text",
     "inputType": "number",
     "isRequired": true,
     "name": "number_total",
     "title": "Total number of rooms",
     "validators": [
      {
       "type": "numeric",
       "text": "Minimum value is 0"
      }
     ],
     "visible": false,
     "visibleIf": "{sprayed} = 'partial' or {sprayed} = 'yes'"
    },
    {
     "type": "text",
     "inputType": "number",
     "isRequired": true,
     "name": "number_sprayed",
     "title": "Rooms fully sprayed",
     "validators": [
      {
       "type": "numeric",
       "text": "Minimum value is 0"
      }
     ],
     "visible": false,
     "visibleIf": "{sprayed} = 'partial' or {sprayed} = 'yes'"
    },
    {
     "type": "text",
     "inputType": "number",
     "isRequired": true,
     "name": "number_unsprayed",
