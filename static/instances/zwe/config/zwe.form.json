     "isRequired": true,
     "choices": [
      {
       "value": "ddt",
       "text": "DDT"
      },
      {
       "value": "insect_o_kill",
       "text": "Insect O Kill"
      },
      {
       "value": "by",
       "text": "By"
      },
      {
       "value": "op",
       "text": "Op"
      },
      {
       "value": "bs",
       "text": "Bs"
      }
     ]
    },
    {
     "type": "text",
     "inputType": "number",
     "isRequired": true,
     "name": "total_population",
     "title": "Total population protected",
     "validators": [
      {
       "type": "numeric",
       "text": "Minimum value is 0"
      }
     ],
     "visible": false,
     "visibleIf": "{sprayed} = 'partial' or {sprayed} = 'yes'"
    },
    {
     "type": "text",
     "inputType": "number",
     "isRequired": true,
