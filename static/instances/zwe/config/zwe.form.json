     "visible": false,
     "visibleIf": "{sprayed} = 'partial' or {sprayed} = 'yes'"
    },
    {
     "type": "text",
     "inputType": "number",
     "isRequired": true,
     "name": "over_five",
     "title": "Population >= 5 YRS",
     "validators": [
      {
       "type": "numeric",
       "text": "Minimum value is 0"
      }
     ],
     "visible": false,
     "visibleIf": "{sprayed} = 'partial' or {sprayed} = 'yes'"
    },
    {
     "type": "text",
     "isRequired": true,
     "name": "name_household",
     "placeHolder": "Name",
     "title": "Head of household name"
    }
   ]
  }
 ]
}