{
  "applets": {
    "irs_plan": {},
    "irs_tasker": {},
    "irs_record_point": {
      "title": "IRS Data Collector",
      "metadata": {
        "show": true
      }
    },
    "irs_monitor": {
      "title": "IRS Dashboard",
      "map": {
        "chart_type": "map",
        "spatial_bins": true,
        "bin_by": "location.selection.id",
        "aggregation_names": [
          "structures sprayed %"
        ],
        "property_layers": [
          "risk"
        ]
      },
      "table": {
        "aggregation_names": [
          "structures sprayed",
          "structures sprayed %"
        ],
        "bin_by": "location.selection.name",
        "chart_type": "table"
      },
      "charts": [
        {
          "id": "team_performance",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Team performance",
              "yaxis": {
                "title": "# of rooms"
              },
              "xaxis": {
                "title": "Teams"
              }
            },
            "chart_type": "bar",
            "bin_by": "team_name",
            "single_series": {
              "aggregation_name": "structures sprayed"
            }
          }
        },
        {
          "id": "room_coverage_by_week",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "chart_type": "bar",
            "time_series": true,
            "cumulative": true,
            "bin_by": "recorded_on",
            "layout": {
              "showlegend": true,
              "title": "Spray room coverage",
              "yaxis": {
                "title": "# of rooms"
              },
              "xaxis": {
                "title": "Period commencing"
              },
              "barmode": "stack"
            },
            "multi_series": [
              {
                "aggregation_name": "structures sprayed",
                "colour": "green"
              },
              {
                "aggregation_name": "sprayable structures not sprayed",
                "colour": "red"
              }
            ]
          }
        },
        {
          "id": "spray_status_absolute",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Spray status",
              "yaxis": {
                "title": "# of households"
              },
              "xaxis": {
                "title": "Spray status"
              }
            },
            "chart_type": "bar",
            "bin_by": "_decorated.sprayed_status",
            "single_series": {
              "aggregation_name": "count"
            }
          }
        },
        {
          "id": "spray_status_pie",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "title": "Sprayed status proportion"
            },
            "chart_type": "pie",
            "bin_by": "_decorated.sprayed_status",
            "series": [
              {
                "aggregation_name": "count"
              }
            ]
          }
        },
        {
          "id": "proportion_sprayed_by_week",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "chart_type": "line",
            "time_series": true,
            "bin_by": "recorded_on",
            "layout": {
              "showlegend": true,
              "title": "Room coverage % of target",
              "yaxis": {
                "title": "% coverage"
              },
              "xaxis": {
                "title": "Period commencing"
              }
            },
            "multi_series": [
              {
                "aggregation_name": "structures sprayed %",
                "colour": "green"
              }
            ]
          }
        }
      ]
    },
    "meta": {},
    "data_wizard": {},
    "debug": {}
  },
  "map_focus": {
    "centre": {
      "lat": -21.73564350618581,
      "lng": 30.083462513044424
    },
    "zoom": 6.5
  },
  "instance": {
    "title": "Zimbabwe IRS Tool",
    "location_name": "Zimbabwe (Matabeleland South)",
    "slug": "zwe"
  },
  "spatial_hierarchy": {
    "data_version": 7,
    "markers": {
      "planning_level_name": "wards",
      "record_location_selection_level_name": "wards",
      "denominator_fields": {
        "number_of_households": "NumHouseholds"
      }
    },
    "levels": [
      {
        "group_by_field": "DISTRICT",
        "field_name": "WARD_ID",
        "display_field_name": "WARD_ID",
        "name": "wards"
      },
      {
        "field_name": "ClstrID",
        "display_field_name": "ClstrID",
        "name": "clusters"
      }
    ]
  }
}
