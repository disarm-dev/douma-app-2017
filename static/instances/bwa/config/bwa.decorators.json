{
  "status": [
    {"blue": "n_rooms_material == room_count"},
    {"red": "any_sprayed == 'no'"},
    {"yellow": "room_count > number_sprayed"},
    {"green": "room_count == number_sprayed"}
  ],
  "sprayed_status": [
    {"not sprayable": "n_rooms_material == room_count"},
    {"not sprayed": "any_sprayed == 'no'"},
    {"partial": "room_count > number_sprayed"},
    {"sprayed": "room_count == number_sprayed"}
  ]
}
