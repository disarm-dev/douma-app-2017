            "layout": {
              "title": "Refusal reason"
            },
            "chart_type": "pie",
            "bin_by": "_decorated.sprayed_status",
            "series": [
              {
                "aggregation_name": "count"
              }
            ]
          }
        }
      ]
    },
    "irs_plan": {
      "table_output": [
        {"display_name": "Village name", "source_field": "name"},
        {"display_name": "Number of households", "source_field": "NumHouseho"},
        {"display_name": "Predicted risk", "source_field": "risk"}
      ]
    },
    "irs_record_point": {
      "metadata": {
        "show": false,
        "optional_fields": []
      }
    },
    "meta": {},
    "data_wizard": {},
    "debug": {}
  },
  "map_focus": {
    "centre": {
      "lat": -19.186677697957833,
      "lng": 16.940917968750004
    },
    "zoom": 6
  },
  "instance": {
    "title": "Namibia IRS Tool",
    "location_name": "Namibia",
    "slug": "nam"
  },
  "spatial_hierarchy": {
    "data_version": 2,
    "markers": {
      "planning_level_name": "villages",
      "record_location_selection_level_name": "villages",
      "denominator_fields": {
        "number_of_households": "NumHouseholds"
      }
    },
    "levels": [
      {
        "group_by_field": "REGION",
        "field_name": "OBJECTID",
        "display_field_name": "CONST",
        "name": "constituencies"
      },
      {
        "group_by_field": "constituency",
        "field_name": "id",
        "display_field_name": "name",
        "name": "villages"
      }
    ]
