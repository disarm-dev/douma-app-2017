        "aggregation_names": [
          "structures sprayed %"
        ],
        "response_point_fields": [
          "recorded_on"
        ]
      },
      "table": {
        "chart_type": "table",
        "bin_by": "location.selection.name",
        "aggregation_names": [
          "structures sprayed",
          "structures sprayed %"
        ]
      },
      "charts": [
        {
          "id": "spray_status_pie",
          "style": {
            "height_constraint": "none",
