{
  "name": "DOUMA",
  "description": "DiSARM Offline Universal Multi-device Application",
  "author": "DiSARM Team",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --content-base src/ --inline --hot --host 0.0.0.0",
